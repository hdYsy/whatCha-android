<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_300"
    android:background="@color/color_move_f4f4f4"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <TextView
        android:id="@+id/bottom_confirm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/dp_10"
        android:text="@string/com_login_gps_sta"
        android:textColor="@color/color_move_00CD58"
        android:textSize="@dimen/sp_18"

        />

    <TextView
        android:id="@+id/bottom_cancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_10"
        android:text="@string/com_login_gps_clo"
        android:textSize="@dimen/sp_18" />
    </RelativeLayout>

    <com.lianjie.andorid.adapter.time.widget.WheelView
        android:id="@+id/main_wheelview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</LinearLayout>
